{
  "name": "whiplash-ui",
  "version": "0.4.7",
  "description": "UI assets for Whiplash Merch",
  "main": "/dist/whiplash-ui.js",
  "repository": "git@github.com:whiplashmerch/whiplash-ui.git",
  "author": "Jacob M. Roufa <jacob.roufa@gmail.com> (http://jacobroufa.com/)",
  "contributors": [
    {
      "name": "Casey Baggz",
      "email": "casey.baggz@gmail.com"
    },
    {
      "name": "Mark Dickson",
      "email": "mark@sitesteaders.com"
    },
    {
      "name": "Matt Smith",
      "email": "azamatsmith@gmail.com"
    }
  ],
  "license": "MIT",
  "scripts": {
    "css": "node-sass scss/whiplash-ui.scss --source-map true -o dist && node-sass scss/whiplash-ui.scss --output-style compressed > dist/whiplash-ui.min.css",
    "js": "cp js/whiplash-ui.js dist/ && minify --output dist/whiplash-ui.min.js js/whiplash-ui.js",
    "dist": "npm run css && npm run js",
    "preversion": "npm run dist"
  },
  "files": [
    "dist",
    "images",
    "fonts",
    "scss"
  ],
  "devDependencies": {
    "node-sass": "^4.5.3",
    "minifier": "^0.8.1"
  }
}
